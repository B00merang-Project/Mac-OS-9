    GtkScrollbar::has_secondary_backward_stepper = 1
    GtkScrollbar::min_slider_length = 15
  
    GtkVScale::slider_length = 16
    GtkVScale::slider_width = 19
    GtkHScale::slider_length = 17
    GtkHScale::slider_width = 19

    GtkEntry::padding = 0

